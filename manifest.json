{
    "manifest_version": 2,
    "name": "AI to Butt",
    "version": "1.0",
    "description": "Replaces instances of 'AI' and 'A.I.' with 'butt' while preserving case sensitivity",
    
    "icons": {
      "48": "icons/butt-48.png",
      "96": "icons/butt-96.png"
    },
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["replace.js"]
      }
    ]
  }
